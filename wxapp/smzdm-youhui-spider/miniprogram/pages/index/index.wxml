<view class="page">
  <view class="youhuiItem" wx:for="{{youhuiLists}}">
    <image src="{{item.img}}" />
    <view class="youhuiInfo">
      <view class="title">
        {{item.title}}
      </view>
      <view class="price">
        {{item.price}}
      </view>
    </view>
    <view class="desc">
      {{item.desc}}
    </view>
  </view>
</view>